{
  "family": "todoapp-backend",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::706712229921:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::706712229921:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "todoapp-backend",
      "image": "706712229921.dkr.ecr.us-east-1.amazonaws.com/todoapp/backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "44.214.106.29"
        },
        {
          "name": "DB_NAME",
          "value": "tododb"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:706712229921:parameter/todoapp/db/password"
        },
        {
          "name": "DB_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:706712229921:parameter/todoapp/db/username"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/todoapp-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
